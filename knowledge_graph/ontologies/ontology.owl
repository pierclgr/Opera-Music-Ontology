                                    owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Lyrics
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :involvesMusicalRole ;
                                    owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                    owl:onClass :VocalScore
                                  ] ;
