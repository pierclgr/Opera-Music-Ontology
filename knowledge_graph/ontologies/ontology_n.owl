            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ocm/ontology/involvesComposition"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/ocm/ontology/Aria"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>AriaPerformance</rdfs:label>
    </owl:Class>
    


